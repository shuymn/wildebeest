{
	"name": "my-qwik-basic-starter",
	"description": "App with Routing built-in (recommended)",
	"type": "module",
	"engines": {
		"node": ">=15.0.0"
	},
	"private": true,
	"scripts": {
		"pretypes-check": "npm run build",
		"types-check": "tsc",
		"lint": "eslint src mock-db adaptors",
		"build": "vite build && vite build -c adaptors/cloudflare-pages/vite.config.ts",
		"dev": "vite --mode ssr",
		"watch": "nodemon -w ./src --ext tsx,ts --exec npm run build"
	},
	"devDependencies": {
		"@builder.io/qwik": "0.21.0",
		"@builder.io/qwik-city": "0.4.0",
		"@types/eslint": "8.44.2",
		"@types/jest": "29.5.3",
		"@types/node": "18.17.4",
		"@types/node-fetch": "latest",
		"@typescript-eslint/eslint-plugin": "6.3.0",
		"@typescript-eslint/parser": "6.3.0",
		"autoprefixer": "10.4.14",
		"eslint": "8.46.0",
		"eslint-plugin-qwik": "0.16.2",
		"jest": "29.6.2",
		"lorem-ipsum": "2.0.8",
		"node-fetch": "3.3.2",
		"nodemon": "3.0.1",
		"postcss": "8.4.27",
		"prettier": "3.0.1",
		"sass": "1.65.1",
		"tailwindcss": "3.3.3",
		"ts-jest": "29.1.1",
		"typescript": "5.1.6",
		"undici": "5.23.0",
		"vite": "4.4.9",
		"vite-tsconfig-paths": "3.6.0"
	},
	"dependencies": {
		"modern-normalize": "2.0.0"
	}
}
